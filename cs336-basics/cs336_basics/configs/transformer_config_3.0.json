{
    "name": "Parallel Layers",
    "description": "",
    "model": {
        "d_model": 512,
        "num_heads": 16,
        "num_layers": 4,
        "d_ff": 2048,
        "context_length": 256,
        "attn_pdrop": 0.1,
        "residual_pdrop": 0.1
    },
    "training": {
        "batch_size": 512,
        "total_iterations": 3000,
        "betas": [0.9, 0.98],
        "eps": 1e-9,
        "alpha_min": 0.001,
        "alpha_max": 0.01,
        "T_warmup": 1500,
        "T_cosine": 3000,
        "weight_decay": 0.1,
        "max_grad_norm": 1.0,
        "log_every": 10,
        "checkpoint_every": 1000
    },
    "tokenizer": {
        "vocab_size": 10000,
        "merges_filename": "tiny_stories_merges.txt",
        "vocab_filename": "tiny_stories_vocab.json",
        "training_data": "TinyStoriesV2-GPT4-train.txt",
        "special_tokens": ["<|endoftext|>"]
    },
    "data": {
        "training_data": "tiny_stories_train",
        "validation_data": "tiny_stories_valid"
    },
    "random_seed": 42
}